c     csup(2) = 8.93
c     csup(3) = 12.21

c     csub(1) = 6.48 
c     csub(2) = 9.28
c     csub(3) = 12.21

c     M_cr = 0.6         ! critical mach number for sphere

c     Sutherlands law for air (moderate), viscosity at particles location
c     c0   = 120.0       ! K
